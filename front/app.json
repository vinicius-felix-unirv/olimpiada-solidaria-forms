{
  "expo": {
    "name": "Olimpíada Solidária",
    "slug": "olimpiada-solidaria-forms",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./public/assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./public/assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "olimpiada-solidaria"
  }
}
